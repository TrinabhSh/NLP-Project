   "source": [
    "temp = []\n",
    "for i in range(7):\n",
    "    km = KMeans(n_clusters = i+1)\n",
    "    km_model = km.fit(x_vector)\n",
    "    output = km_model.inertia_\n",
    "    temp.append(output)\n",
    "    \n",
    "plt.plot(range(7), temp, marker='o')\n",
    "plt.xlabel('Number of clusters')\n",
    "plt.ylabel('Distortion')\n",
    "plt.show()"
   ]
  },
